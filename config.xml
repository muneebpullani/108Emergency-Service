<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ONEEmergencyService.ONESS" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>108EmergencyService</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <splash src="res/screen/android/splash640.png" density="port-hdpi"/>
        <splash src="res/screen/android/splash426.png" density="port-ldpi"/>
        <splash src="res/screen/android/splash470.png" density="port-mdpi"/>
        <splash src="res/screen/android/splash960.png" density="port-xhdpi"/>
        <preference name="AutoHideSplashScreen" value="true" />
        <preference name="SplashShowOnlyFirstTime" value="true" />
        <preference name="SplashScreenDelay" value="3000" />
        <icon src="res/android/36.png" density="ldpi" />
        <icon src="res/android/48.png" density="mdpi" />
        <icon src="res/android/72.png" density="hdpi" />
        <icon src="res/android/96.png" density="xhdpi" />
        <icon src="res/android/144.png" density="xxhdpi" />
        <icon src="res/android/192.png" density="xxxhdpi" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
